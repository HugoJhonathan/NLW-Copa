        "android": {
          "buildType": "apk"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.hugojhonathan.nlwmobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3b6f3b9c-2833-4b82-a32e-b554b12f4809"
      }
    }
  }
